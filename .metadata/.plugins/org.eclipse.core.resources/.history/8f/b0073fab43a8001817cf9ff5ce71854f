package com.im.test;

public class Parent {

	public Parent(int i)
	{
		System.out.println("init parent");
	}
	
	public static void main(String args[])
	{
		Son son=new Son();
	}
}
class Child extends Parent{
	
	public Child(int i)
	{
		System.out.println("init child");
	}
}

class Son extends Child{
	
	public Son(int i)
	{
		System.out.println("init Son");
	}
}